{
  "name": "@thecapylab/capykit",
  "version": "0.1.0",
  "description": "CapyKit is an opinionated Vue Component Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheCapyLab/CapyKit.git"
  },
  "keywords": [
    "Vue",
    "Component",
    "Library"
  ],
  "author": "TheCapyLab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheCapyLab/CapyKit/issues"
  },
  "type": "module",
  "homepage": "https://github.com/TheCapyLab/CapyKit#readme",
  "scripts": {
    "dev:clean": "node scripts/clean.js",
    "dev:install:all": "node scripts/install-all.js",
    "build:all": "node scripts/build.js",
    "build:capykit": "node scripts/build.js capykit",
    "build:core": "node scripts/build.js core",
    "build:composables": "node scripts/build.js composables",
    "pack:all": "node scripts/pack.js --collect",
    "pack:capykit": "node scripts/pack.js capykit",
    "pack:core": "node scripts/pack.js core",
    "pack:composables": "node scripts/pack.js composables",
    "create:package": "node scripts/create-package.js",
    "delete:package": "node scripts/delete-package.js",
    "package:create": "node scripts/create-package.js",
    "package:delete": "node scripts/delete-package.js",
    "version:patch:increment": "node scripts/increment-version.js patch",
    "version:minor:increment": "node scripts/increment-version.js minor",
    "version:major:increment": "node scripts/increment-version.js major",
    "version:set": "node scripts/set-version.js",
    "docs:dev": "npm run docs:update-all && vitepress dev docs",
    "docs:build": "npm run docs:update-all && vitepress build docs",
    "docs:preview": "npm run docs:update-all && vitepress preview docs",
    "docs:update-all": "node docs/scripts/update-versions.cjs && node docs/scripts/generate-sidebar.cjs && node docs/scripts/show-structure.cjs",
    "docs:update-versions": "node docs/scripts/update-versions.cjs",
    "docs:update-sidebar": "node docs/scripts/generate-sidebar.cjs",
    "docs:show-structure": "node docs/scripts/show-structure.cjs"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "glob": "^11.0.3",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.22"
  }
}
